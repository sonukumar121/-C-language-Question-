#include<stdio.h>
int main()
// input a[5]={1,2,3,4,5}-->output-->input element present in array or not.
{
  int a[5],i,x,low=0,high=4,mid;
  printf("Enter the five number= ");
  for(i=0;i<=4;i++)
  {
    scanf("%d",&a[i]);
  }
  printf("Enter the searching elements= ");
  for(i=0;i<=0;i++)
  {
    scanf("%d",&x);
  }
 while(low<=high) //-->0<=4
 {
     mid=high+low/2;//-->mid=2


   if(x==a[mid])
   {
       printf("Elemets is found");
       break;
   }

   else if(x>a[mid])
   {
       low=mid+1;
   }
   else
   {
       high=mid-1;
   }
 }
     if(low>=high)
 {
     printf("Elements is not found");
 }
}
















